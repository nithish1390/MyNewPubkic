// Read <pd:startName> and <pd:endName>
NodeList startNameNodes = inputDoc.getElementsByTagName("pd:startName");
if (startNameNodes.getLength() > 0) {
    startNode = startNameNodes.item(0).getTextContent().trim();
}

NodeList endNameNodes = inputDoc.getElementsByTagName("pd:endName");
if (endNameNodes.getLength() > 0) {
    endNode = endNameNodes.item(0).getTextContent().trim();
}
